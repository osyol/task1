class Main {
    public static void main(String[] args) {
        java.util.Scanner sc = new java.util.Scanner(System.in);

        // Ввод строки
        System.out.println("Enter your string:");
        String str = sc.nextLine();

        int n = 2; //

        // Разделение слов по пробелам
        String[] words = str.split("\\s+");

        String[] unique = new String[words.length]; 
        int[] counts = new int[words.length];
        int uniqueCount = 0;

        // Подсчёт слов
        for (String w : words) {
            int index = -1;

            for (int i = 0; i < uniqueCount; i++) {
                if (unique[i].equals(w)) {
                    index = i;
                    break;
                }
            }

            if (index == -1) {
                unique[uniqueCount] = w;
                counts[uniqueCount] = 1;
                uniqueCount++;
            } else {
                counts[index]++;
            }
        }

        // Вывод слов встречающихся минимум 2 раза
        System.out.println("\nWords that you wrote twice or more times:");
        for (int i = 0; i < uniqueCount; i++) {
            if (counts[i] > n) {
                System.out.println(unique[i] + " = " + counts[i]);
            }
        }

        sc.close();
    }
}